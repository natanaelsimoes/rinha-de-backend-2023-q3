{
  "name": "natanaelsimoes-rinha",
  "version": "1.0.0",
  "description": "API para a Rinha de Backend 2023 Q3",
  "main": "dist/index.js",
  "scripts": {
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run start\"",
    "start": "ENV=dev ts-node src/index.ts",
    "prod": "npm run migrate && node ./index.js",
    "build": "rm -rf dist && npm run lint && tsc",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts ./src",
    "test": "jest",
    "migrate": "./node_modules/.bin/typeorm migration:run -d ./datasource.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natanaelsimoes/rinha-de-backend-2023-q3.git"
  },
  "keywords": [
    "rinha-de-backend",
    "api",
    "node",
    "fastify"
  ],
  "author": "Natanael Sim√µes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natanaelsimoes/rinha-de-backend-2023-q3/issues"
  },
  "homepage": "https://github.com/natanaelsimoes/rinha-de-backend-2023-q3#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.46.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "fastify": "^4.21.0",
    "pg": "^8.11.2",
    "redis": "^4.6.7",
    "typeorm": "^0.3.17"
  }
}
